{
  "name": "noaesperanza-appB",
  "version": "1.0.0",
  "description": "App B com estrutura clínica e rotas públicas do appC",
  "main": "src/main.tsx",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "serve": "node server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "test:cypress:component": "cypress run --component",
    "test:cypress:e2e": "cypress run --e2e",
    "sonar": "sonar-scanner",
    "quality": "npm run lint && npm run type-check && npm run test:coverage && npm run test:cypress && npm run sonar",
    "quality:quick": "npm run lint && npm run type-check && npm run test",
    "coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "lint:staged": "lint-staged",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "pre-commit": "npm run lint:staged && npm run type-check",
    "pre-push": "npm run quality:quick"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.57.4",
    "@types/react-router-dom": "^5.3.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.344.0",
    "mammoth": "^1.11.0",
    "pdf-parse": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.30.1",
    "shadcn-ui": "^0.9.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.3.2",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.14",
    "cypress": "^15.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.24",
    "prettier": "^3.2.5",
    "sonar-scanner": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
